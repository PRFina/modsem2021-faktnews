@prefix : <http://www.modsem.org/fakt-news#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix obda: <https://w3id.org/obda/vocabulary#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.modsem.org/fakt-news> .

<http://www.modsem.org/fakt-news> rdf:type owl:Ontology ;
                                   owl:imports <http://www.ontologydesignpatterns.org/cp/owl/set.owl> ,
                                               <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl> ;
                                   dcterms:creator "Lorenzo Tabasso"@it ,
                                                   "Pio Raffaele Fina"@it ;
                                   dcterms:description "An Ontology to describe and annotate the fact checking process."@en ;
                                   vann:preferredNamespacePrefix "fakt"@en ;
                                   vann:preferredNamespaceUri "http://www.modsem.org/fakt-news"@en ;
                                   rdfs:label "Fakt News"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty ;
                rdfs:comment "An entity responsible for making the resource."@en ;
                rdfs:label "creator"@en .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty ;
                    rdfs:comment "An account of the resource."@en ;
                    rdfs:label "description"@en .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty ;
                              rdfs:comment "The preferred namespace prefix to use when using terms from this vocabulary in an XML document."@en ;
                              rdfs:label "preferredNamespacePrefix"@en .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty ;
                           rdfs:comment "The preferred namespace URI to use when using terms from this vocabulary in an XML document."@en ;
                           rdfs:label "preferredNamespaceUri"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.modsem.org/fakt-news#affiliatedTo
:affiliatedTo rdf:type owl:ObjectProperty ;
              rdfs:domain prov:Agent ;
              rdfs:range prov:Agent .


###  http://www.modsem.org/fakt-news#associatedReview
:associatedReview rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf :claimReviewed ;
                  rdfs:domain :Claim ;
                  rdfs:range :Review .


###  http://www.modsem.org/fakt-news#claimReviewed
:claimReviewed rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Review ;
               rdfs:range :Claim ;
               owl:propertyChainAxiom ( :wasOutputOfClaimEvaluation
                                        :hasPreviousActivity
                                        prov:used
                                      ) .


###  http://www.modsem.org/fakt-news#create
:create rdf:type owl:ObjectProperty ;
        owl:inverseOf :creator .


###  http://www.modsem.org/fakt-news#createCodeOfPrinciples
:createCodeOfPrinciples rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :create ;
                        rdfs:domain :FactCheckingAuthority ;
                        rdfs:range :CodeOfPrinciples .


###  http://www.modsem.org/fakt-news#creator
:creator rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf prov:wasAttributedTo ;
         rdfs:domain schema:CreativeWork ;
         rdfs:comment "The entity who creates the resource."@en ;
         rdfs:label "creator"@en .


###  http://www.modsem.org/fakt-news#derivedFromSource
:derivedFromSource rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf prov:wasAttributedTo ;
                   owl:inverseOf :isSourceOfEvidence ;
                   rdfs:domain :Evidence ;
                   rdfs:range :EvidenceSource .


###  http://www.modsem.org/fakt-news#hasActivity
:hasActivity rdf:type owl:ObjectProperty ;
             owl:inverseOf :isActivityOfPlan ;
             rdf:type owl:InverseFunctionalProperty ;
             rdfs:domain prov:Plan ;
             rdfs:range prov:Activity .


###  http://www.modsem.org/fakt-news#hasCapacity
:hasCapacity rdf:type owl:ObjectProperty ;
             rdfs:domain prov:Agent .


###  http://www.modsem.org/fakt-news#hasEvidence
:hasEvidence rdf:type owl:ObjectProperty ;
             rdfs:domain :Review ;
             rdfs:range :Evidence .


###  http://www.modsem.org/fakt-news#hasJudgment
:hasJudgment rdf:type owl:ObjectProperty ;
             rdfs:domain :Review ;
             rdfs:range :Judgment .


###  http://www.modsem.org/fakt-news#hasNextActivity
:hasNextActivity rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf :hasPreviousActivity ;
                 rdf:type owl:TransitiveProperty ;
                 rdfs:domain prov:Activity ;
                 rdfs:range prov:Activity .


###  http://www.modsem.org/fakt-news#hasPreviousActivity
:hasPreviousActivity rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:TransitiveProperty ;
                     rdfs:domain prov:Activity ;
                     rdfs:range prov:Activity .


###  http://www.modsem.org/fakt-news#hasRating
:hasRating rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Review ;
           rdfs:range :Rating .


###  http://www.modsem.org/fakt-news#hasTimeInterval
:hasTimeInterval rdf:type owl:ObjectProperty ;
                 rdfs:domain prov:Activity ;
                 rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> .


###  http://www.modsem.org/fakt-news#isActivityOfPlan
:isActivityOfPlan rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain prov:Activity ;
                  rdfs:range prov:Plan .


###  http://www.modsem.org/fakt-news#isCertificatedBy
:isCertificatedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain :FactCheckingOrganization ;
                  rdfs:range :FactCheckingAuthority ;
                  owl:propertyChainAxiom ( :observeCodeOfPrinciples
                                           :creator
                                         ) .


###  http://www.modsem.org/fakt-news#isClaimedBy
:isClaimedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :creator ;
             rdfs:domain :Claim .


###  http://www.modsem.org/fakt-news#isFirstActivityOfPlan
:isFirstActivityOfPlan rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :isActivityOfPlan .


###  http://www.modsem.org/fakt-news#isLastActivityOfPlan
:isLastActivityOfPlan rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :isActivityOfPlan .


###  http://www.modsem.org/fakt-news#isReviewedBy
:isReviewedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :creator ;
              rdfs:domain :Review .


###  http://www.modsem.org/fakt-news#isSourceOfEvidence
:isSourceOfEvidence rdf:type owl:ObjectProperty .


###  http://www.modsem.org/fakt-news#isUsedByClaimEvaluation
:isUsedByClaimEvaluation rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf prov:used ;
                         rdfs:domain :ClaimEvaluationActivity ;
                         rdfs:range :Evidence .


###  http://www.modsem.org/fakt-news#isUsedBySelection
:isUsedBySelection rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf prov:used ;
                   rdfs:domain :SelectionActivity ;
                   rdfs:range :AlternativesSet .


###  http://www.modsem.org/fakt-news#isUsedByVerification
:isUsedByVerification rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf prov:used ;
                      rdfs:domain :VerificationActivity ;
                      rdfs:range :Claim .


###  http://www.modsem.org/fakt-news#observeCodeOfPrinciples
:observeCodeOfPrinciples rdf:type owl:ObjectProperty ;
                         rdfs:domain :FactCheckingOrganization ;
                         rdfs:range :CodeOfPrinciples ;
                         rdfs:comment "object property da asserire quando un authority verifica che i suoi principi vengano osservati" ;
                         skos:example "https://www.poynter.org/ifcn-fact-checkers-code-of-principles/"@en .


###  http://www.modsem.org/fakt-news#supportedBy
:supportedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Evidence ;
             rdfs:range :DigitalDocument ;
             rdfs:comment "A review is supported by a materialization of some sort of document (for example a subtype of creative work)."@en .


###  http://www.modsem.org/fakt-news#wasOutputOfClaimEvaluation
:wasOutputOfClaimEvaluation rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf prov:wasGeneratedBy ;
                            rdfs:domain :Review ;
                            rdfs:range :ClaimEvaluationActivity .


###  http://www.modsem.org/fakt-news#wasOutputOfSelection
:wasOutputOfSelection rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf prov:wasGeneratedBy ;
                      rdfs:domain :Claim ;
                      rdfs:range :SelectionActivity .


###  http://www.modsem.org/fakt-news#wasOutputOfVerification
:wasOutputOfVerification rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf prov:wasGeneratedBy ;
                         rdfs:domain :Evidence ;
                         rdfs:range :VerificationActivity .


###  http://www.w3.org/ns/prov#agent
prov:agent rdf:type owl:ObjectProperty ;
           rdfs:domain prov:Association ;
           rdfs:range prov:Agent .


###  http://www.w3.org/ns/prov#hadRole
prov:hadRole rdf:type owl:ObjectProperty ;
             rdfs:domain prov:Association ;
             rdfs:range prov:Role .


###  http://www.w3.org/ns/prov#qualifiedAssociation
prov:qualifiedAssociation rdf:type owl:ObjectProperty ;
                          rdfs:domain prov:Activity ;
                          rdfs:range prov:Association .


###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty ;
          rdfs:domain prov:Activity ;
          rdfs:range prov:Entity .


###  http://www.w3.org/ns/prov#wasAssociatedWith
prov:wasAssociatedWith rdf:type owl:ObjectProperty ;
                       rdfs:domain prov:Activity ;
                       rdfs:range prov:Agent .


###  http://www.w3.org/ns/prov#wasAttributedTo
prov:wasAttributedTo rdf:type owl:ObjectProperty ;
                     rdfs:domain prov:Entity ;
                     rdfs:range prov:Agent .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain prov:Entity ;
                    rdfs:range prov:Activity .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/date
dcterms:date rdf:type owl:DatatypeProperty ;
             rdfs:comment "A point or period of time associated with an event in the lifecycle of the resource."@en ;
             rdfs:label "date"@en .


###  http://purl.org/dc/terms/language
dcterms:language rdf:type owl:DatatypeProperty ;
                 rdfs:domain schema:CreativeWork ;
                 rdfs:range xsd:string ;
                 rdfs:comment "A language of the resource."@en ;
                 rdfs:label "language"@en .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:comment "A name given to the resource."@en ;
              rdfs:label "title"@en .


###  http://schema.org/name
schema:name rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:string ;
            rdfs:comment "The name of the item."@en ;
            rdfs:label "name"@en .


###  http://schema.org/url
schema:url rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:anyURI ;
           rdfs:comment "The URL of an item."@en ;
           rdfs:label "url"@en ;
           skos:altLabel "URL"@en .


###  http://www.modsem.org/fakt-news#capacityType
:capacityType rdf:type owl:DatatypeProperty ;
              rdfs:domain :EvidenceSource ;
              rdfs:range [ rdf:type rdfs:Datatype ;
                           owl:oneOf [ rdf:type rdf:List ;
                                       rdf:first "Citizen" ;
                                       rdf:rest [ rdf:type rdf:List ;
                                                  rdf:first "Civil Service" ;
                                                  rdf:rest [ rdf:type rdf:List ;
                                                             rdf:first "Company" ;
                                                             rdf:rest [ rdf:type rdf:List ;
                                                                        rdf:first "Expert" ;
                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                   rdf:first "Government" ;
                                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                                              rdf:first "Lobby" ;
                                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                                         rdf:first "NGO" ;
                                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                                    rdf:first "Opposition" ;
                                                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                                                               rdf:first "Think Tank" ;
                                                                                                                               rdf:rest rdf:nil
                                                                                                                             ]
                                                                                                                  ]
                                                                                                       ]
                                                                                            ]
                                                                                 ]
                                                                      ]
                                                           ]
                                                ]
                                     ]
                         ] ;
              rdfs:comment "The Evidence Source social function indipendent of the context."@en ;
              rdfs:label "capacity"@en ;
              skos:altLabel "Position"@en .


###  http://www.modsem.org/fakt-news#claimContent
:claimContent rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :documentContent ;
              rdfs:domain :Claim ;
              rdfs:range xsd:string ;
              rdfs:comment "The main content of a claim: what claimant said."@en ;
              rdfs:label "claim content"@en .


###  http://www.modsem.org/fakt-news#documentContent
:documentContent rdf:type owl:DatatypeProperty ;
                 rdfs:domain :DigitalDocument ;
                 rdfs:comment "The main content of the digital document."@en ;
                 rdfs:label "document content"@en .


###  http://www.modsem.org/fakt-news#judgmentSummary
:judgmentSummary rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Judgment ;
                 rdfs:range xsd:string ;
                 rdfs:comment "A brief description of the judgment given by some fact checker."@en ;
                 rdfs:label "judgment summary"@en ;
                 skos:altLabel "judgment description"@en .


###  http://www.modsem.org/fakt-news#mention
:mention rdf:type owl:DatatypeProperty ;
         rdfs:domain :Review ;
         rdfs:range xsd:anyURI ;
         rdfs:comment "A mention to some entity in a review."@en ;
         rdfs:label "mention"@en .


###  http://www.modsem.org/fakt-news#ratingAssociatedMedia
:ratingAssociatedMedia rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Rating ;
                       rdfs:range xsd:anyURI ;
                       rdfs:comment "A visual representation of the rating value."@en ;
                       rdfs:label "rating associated media"@en ;
                       skos:altLabel "rating depiction"@en .


###  http://www.modsem.org/fakt-news#ratingSystemComment
:ratingSystemComment rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Rating ;
                     rdfs:range xsd:string ;
                     rdfs:comment "A comment on the rating system."@en ;
                     rdfs:label "rating system comment"@en .


###  http://www.modsem.org/fakt-news#ratingValue
:ratingValue rdf:type owl:DatatypeProperty ;
             rdfs:domain :Rating ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "False" ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "Mostly False" ;
                                                 rdf:rest [ rdf:type rdf:List ;
                                                            rdf:first "Mostly True" ;
                                                            rdf:rest [ rdf:type rdf:List ;
                                                                       rdf:first "True" ;
                                                                       rdf:rest rdf:nil
                                                                     ]
                                                          ]
                                               ]
                                    ]
                        ] ;
             rdfs:comment "The rating value that encode a final verdict about the claim truthfulness."@en ;
             rdfs:label "rating value"@en .


###  http://www.modsem.org/fakt-news#reviewContent
:reviewContent rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :documentContent ;
               rdfs:domain :Review ;
               rdfs:range xsd:string ;
               rdfs:comment "The content of a review article."@en ;
               rdfs:label "review content"@en .


###  http://www.modsem.org/fakt-news#tag
:tag rdf:type owl:DatatypeProperty ;
     rdfs:domain :Review ;
     rdfs:range xsd:string ;
     rdfs:comment "A topic tag of the review."@en ;
     rdfs:label "tag"@en ;
     skos:altLabel "topic tag"@en .


#################################################################
#    Classes
#################################################################

###  http://schema.org/CreativeWork
schema:CreativeWork rdf:type owl:Class ;
                    owl:equivalentClass :DigitalDocument ;
                    rdfs:subClassOf prov:Entity ;
                    rdfs:comment "The most generic kind of creative work, including books, movies, photographs, software programs, etc."@en ;
                    rdfs:label "Creative Work"@en .


###  http://www.modsem.org/fakt-news#AlternativesSet
:AlternativesSet rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/set.owl#Set> ;
                 rdfs:comment "A set of claims from which the fact-checker chooses the claim to verify."@en ;
                 rdfs:label "Alternatives Set"@en .


###  http://www.modsem.org/fakt-news#Author
:Author rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :Claimant
                                            :FactChecker
                                          )
                            ] ;
        rdfs:subClassOf prov:Role ;
        rdfs:comment "An author of a digital document. It could be for example a Fact-Checker or a Claim Author."@en ;
        rdfs:label "Author"@en .


###  http://www.modsem.org/fakt-news#Claim
:Claim rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :isClaimedBy ;
                                                    owl:someValuesFrom prov:Agent
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :wasOutputOfSelection ;
                                                    owl:allValuesFrom :SelectionActivity
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :DigitalDocument ;
       rdfs:comment "A statement by an entity about some fact."@en ;
       rdfs:label "Claim"@en ;
       skos:altLabel "Statement"@en .


###  http://www.modsem.org/fakt-news#ClaimEvaluationActivity
:ClaimEvaluationActivity rdf:type owl:Class ;
                         rdfs:subClassOf prov:Activity ;
                         rdfs:comment "Last activity of fact checking process. In this activity fact checkers produces a piece of work, the review, about one or more claims supported by one or more piece of evidences."^^xsd:string ;
                         rdfs:label "Claim Evaluation Activity"@en .


###  http://www.modsem.org/fakt-news#Claimant
:Claimant rdf:type owl:Class ;
          rdfs:subClassOf prov:Role ;
          rdfs:comment "The entity who state the claim."@en ;
          rdfs:label "Claimant"@en .


###  http://www.modsem.org/fakt-news#CodeOfPrinciples
:CodeOfPrinciples rdf:type owl:Class ;
                  rdfs:subClassOf prov:Entity ;
                  rdfs:comment "A series of commitments organizations abide by to promote excellence in fact-checking."@en ;
                  rdfs:label "Code of Principles"@en .


###  http://www.modsem.org/fakt-news#ConfirmedFact
:ConfirmedFact rdf:type owl:Class ;
               rdfs:subClassOf :Judgment ;
               rdfs:comment "The claim content presents facts totally supported by evidence."@en ;
               rdfs:label "Confirmed Fact"@en .


###  http://www.modsem.org/fakt-news#DeceptiveEditing
:DeceptiveEditing rdf:type owl:Class ;
                  rdfs:subClassOf :Judgment ;
                  rdfs:comment "The claim content presents facts that has been edited and rearranged in such way that ​likely to make the receiver believe something that is not true."@en ;
                  rdfs:label "Deceptive Editing"@en .


###  http://www.modsem.org/fakt-news#DigitalDocument
:DigitalDocument rdf:type owl:Class ;
                 rdfs:subClassOf prov:Entity .


###  http://www.modsem.org/fakt-news#DisprovingEvidence
:DisprovingEvidence rdf:type owl:Class ;
                    rdfs:subClassOf :Evidence ;
                    owl:disjointWith :SupportingEvidence ;
                    rdfs:comment "A disproving evidence on a certain claim."@en ;
                    rdfs:label "Disproving Evidence"@en .


###  http://www.modsem.org/fakt-news#Doctoring
:Doctoring rdf:type owl:Class ;
           rdfs:subClassOf :MaliciousTransfomation ;
           rdfs:comment "Altering the content of a claim adding or deleting visual, audio or textual information can deceive the receiver."@en ;
           rdfs:label "Doctoring"@en .


###  http://www.modsem.org/fakt-news#Editor
:Editor rdf:type owl:Class ;
        rdfs:subClassOf prov:Role ;
        rdfs:comment "The entity who manipulate the data in the Verification Activity. It could be the same entity as the Fact Checker."@en ;
        rdfs:label "Editor"@en .


###  http://www.modsem.org/fakt-news#Evidence
:Evidence rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          owl:disjointUnionOf ( :DisprovingEvidence
                                :SupportingEvidence
                              ) ;
          rdfs:comment "An evidence about a claim. Used in the verification stage of the fact-checking process."@en ;
          rdfs:label "Evidence"@en .


###  http://www.modsem.org/fakt-news#EvidenceSource
:EvidenceSource rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :isSourceOfEvidence ;
                                      owl:someValuesFrom :Evidence
                                    ] ;
                rdfs:subClassOf prov:Agent ;
                rdfs:comment "The source of the facts. It could be the entity that states the fact itself or reports it."@en ;
                rdfs:label "Evidence Source"@en .


###  http://www.modsem.org/fakt-news#Fabrication
:Fabrication rdf:type owl:Class ;
             rdfs:subClassOf :MaliciousTransfomation ;
             rdfs:comment "Using Artificial Intelligence to create high-quality content as fake language content, fake images, fake video, simulates audio and convincingly swaps out background images."@en ;
             rdfs:label "Fabrication"@en .


###  http://www.modsem.org/fakt-news#FactChecker
:FactChecker rdf:type owl:Class ;
             rdfs:subClassOf prov:Role ;
             rdfs:comment "The entity that produces a review of a claim given the claim itself and the evidence supporting it."@en ;
             rdfs:label "Fact Checker"@en .


###  http://www.modsem.org/fakt-news#FactCheckingAuthority
:FactCheckingAuthority rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:oneOf ( :A-1-IFCN
                                                         :A-13-DUKES-REPORTER-LAB
                                                       )
                                           ] ;
                       rdfs:subClassOf :Organization ;
                       rdfs:comment "The Fact Checking autority that monitors trends, formats and policy-making about fact-checking worldwide. It promotes and checks the committment of the fact checking organisations to ethical code standards."@en ;
                       rdfs:label "Fact Checking Authority"@en .


###  http://www.modsem.org/fakt-news#FactCheckingOrganization
:FactCheckingOrganization rdf:type owl:Class ;
                          rdfs:subClassOf :Organization ;
                          rdfs:comment "An organization that provides fact-checking services."@en ;
                          rdfs:label "Fact Checking Organisation"@en .


###  http://www.modsem.org/fakt-news#FactCheckingProcess
:FactCheckingProcess rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( prov:Plan
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasActivity ;
                                                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :ClaimEvaluationActivity
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasActivity ;
                                                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :SelectionActivity
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasActivity ;
                                                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass :VerificationActivity
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:comment "The whole activities in which given a selected claim, supported or disproved by some evidence, generates a review article on the claim truthfulness."@en ;
                     rdfs:label "Fact Checking Process"@en .


###  http://www.modsem.org/fakt-news#Isolation
:Isolation rdf:type owl:Class ;
           rdfs:subClassOf :MissingContext ;
           rdfs:comment "Sharing a brief excerpt  from a longer story creates a false narrative that does not reflect the event as it occurred."@en ;
           rdfs:label "Isolation"@en .


###  http://www.modsem.org/fakt-news#Judgment
:Judgment rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          rdfs:comment "A concise judgemental statement given by the fact checker to motivate the review rating"@en ;
          rdfs:label "Judgment"@en ;
          skos:altLabel "Justification"@en ,
                        "Reason"@en .


###  http://www.modsem.org/fakt-news#MaliciousTransfomation
:MaliciousTransfomation rdf:type owl:Class ;
                        rdfs:subClassOf :Judgment ;
                        rdfs:comment "Part or all of the claim content has been manipulated to transform the content itself."@en ;
                        rdfs:label "Malicious Transformation"@en .


###  http://www.modsem.org/fakt-news#Misrepresentation
:Misrepresentation rdf:type owl:Class ;
                   rdfs:subClassOf :MissingContext ;
                   rdfs:comment "Presenting unaltered content in an inaccurate manner misrepresents the content itself and misleads the receiver"@en ;
                   rdfs:label "Misrepresentation"@en .


###  http://www.modsem.org/fakt-news#MissingContext
:MissingContext rdf:type owl:Class ;
                rdfs:subClassOf :Judgment ;
                rdfs:comment "The claim content lacks or misstates the context in which events occurred."@en ;
                rdfs:label "Missing Context"@en .


###  http://www.modsem.org/fakt-news#NotClearGeographyContext
:NotClearGeographyContext rdf:type owl:Class ;
                          rdfs:subClassOf :Misrepresentation ;
                          rdfs:comment "The geographic context is incorrect or not clear such as event incorrect location."@en ;
                          rdfs:label "Not Clear Geography Context"@en ;
                          skos:altLabel "Misrepresented Geography Context"@en .


###  http://www.modsem.org/fakt-news#NotClearPeriodContext
:NotClearPeriodContext rdf:type owl:Class ;
                       rdfs:subClassOf :Misrepresentation ;
                       rdfs:comment "The time context is incorrect or not clear such as event incorrect date."@en ;
                       rdfs:label "Not Clear Time Period Context"@en ;
                       skos:altLabel "Misrepresented Time Period Context"@en .


###  http://www.modsem.org/fakt-news#NotClearSubject
:NotClearSubject rdf:type owl:Class ;
                 rdfs:subClassOf :Misrepresentation ;
                 rdfs:comment "The subject entity of the claim is incorrect or not clear."@en ;
                 rdfs:label "Not Clear Subject"@en ;
                 skos:altLabel "Misrepresented Subject"@en .


###  http://www.modsem.org/fakt-news#Omission
:Omission rdf:type owl:Class ;
          rdfs:subClassOf :DeceptiveEditing ;
          rdfs:comment "Editing out large portions from a video and presenting it as a complete narrative, despite missing key elements, produce a skew reality."@en ;
          rdfs:label "Omission"@en .


###  http://www.modsem.org/fakt-news#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf prov:Agent ;
              rdfs:comment "A group of persons grouped together or organized for some purpose or work."@en ;
              rdfs:label "Organization"@en .


###  http://www.modsem.org/fakt-news#Person
:Person rdf:type owl:Class ;
        owl:equivalentClass foaf:Person ;
        rdfs:subClassOf prov:Agent ;
        rdfs:comment "A real human person."@en ;
        rdfs:label "Person"@en .


###  http://www.modsem.org/fakt-news#QualitativeClaim
:QualitativeClaim rdf:type owl:Class ;
                  rdfs:subClassOf :Judgment ;
                  rdfs:comment "The claim use qualitative information in place of quantitative descriptions that are open to the receiver interpretation."^^xsd:string ;
                  rdfs:label "Qualitative Claim"@en ;
                  skos:example "\"... in my past five years as leader, I have increased employment.\" is a qualitative claim. This could corresponds to a quantitive information as \"In the past five years, employment has risen from 95.0% to 95.05%.\""^^xsd:string .


###  http://www.modsem.org/fakt-news#Rating
:Rating rdf:type owl:Class ;
        rdfs:subClassOf prov:Entity ;
        rdfs:comment "A rating of a review within a known evaluation meter. The rating value can belong to an external rating system."@en ;
        rdfs:label "Rating"@en .


###  http://www.modsem.org/fakt-news#Review
:Review rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :isReviewedBy ;
                                                     owl:someValuesFrom prov:Agent
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :wasOutputOfClaimEvaluation ;
                                                     owl:allValuesFrom :ClaimEvaluationActivity
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :DigitalDocument ;
        rdfs:comment "The final document that contains the review of a claim by a fact-checker."^^xsd:string ;
        rdfs:label "Review"@en .


###  http://www.modsem.org/fakt-news#SelectionActivity
:SelectionActivity rdf:type owl:Class ;
                   rdfs:subClassOf prov:Activity ;
                   rdfs:comment "First activity of fact checking process. In this activity fact checkers select one or more claims from a list of suggested alternatives guided by the selection policy."@en ;
                   rdfs:label "Selection Activity"@en .


###  http://www.modsem.org/fakt-news#SoftwareAgent
:SoftwareAgent rdf:type owl:Class ;
               rdfs:subClassOf prov:Agent ;
               rdfs:comment "A software program that act in automounus way in some kind of environment to achive some kind of task."@en ;
               rdfs:label "Software Agent"@en ;
               skos:altLabel "Bot, Software"@en .


###  http://www.modsem.org/fakt-news#Splicing
:Splicing rdf:type owl:Class ;
          rdfs:subClassOf :DeceptiveEditing ;
          rdfs:comment "Editing together different excerpts fundamentally alters the story that is being told. An example is glueing together quotes without context."@en ;
          rdfs:label "Splicing"@en .


###  http://www.modsem.org/fakt-news#SupportingEvidence
:SupportingEvidence rdf:type owl:Class ;
                    rdfs:subClassOf :Evidence ;
                    rdfs:comment "A proving evidence on a certain claim."@en ;
                    rdfs:label "Supporting Evidence"@en .


###  http://www.modsem.org/fakt-news#TrustedSource
:TrustedSource rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:unionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :capacityType ;
                                                     owl:hasValue "Government"
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :capacityType ;
                                                     owl:hasValue "NGO"
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :capacityType ;
                                                     owl:hasValue "Think Tank"
                                                   ]
                                                 )
                                   ] ;
               rdfs:subClassOf :EvidenceSource ;
               rdfs:comment "A Trusted Source is known to be reliable because its authority or authenticity is widely recognized by experts in the field."@en ;
               rdfs:label "Trusted Source"@en ;
               skos:altLabel "Authoritative Source"@en .


###  http://www.modsem.org/fakt-news#VerificationActivity
:VerificationActivity rdf:type owl:Class ;
                      rdfs:subClassOf prov:Activity ;
                      rdfs:comment "In this activity fact checkers searches piece of evidences to verify the validity of a given claim"@en ;
                      rdfs:label "Verification Activity"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdfs:subClassOf prov:Entity .


###  http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval
<http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> rdfs:subClassOf prov:Entity .


###  http://www.w3.org/ns/prov#Activity
prov:Activity rdf:type owl:Class ;
              rdfs:comment "An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities."@en ;
              rdfs:label "Activity"@en .


###  http://www.w3.org/ns/prov#Agent
prov:Agent rdf:type owl:Class ;
           rdfs:comment "An agent is something that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent's activity."@en ;
           rdfs:label "Agent"@en .


###  http://www.w3.org/ns/prov#Association
prov:Association rdf:type owl:Class ;
                 rdfs:comment "An activity association is an assignment of responsibility to an agent for an activity, indicating that the agent had a role in the activity. It further allows for a plan to be specified, which is the plan intended by the agent to achieve some goals in the context of this activity."@en ;
                 rdfs:label "Association"@en .


###  http://www.w3.org/ns/prov#Entity
prov:Entity rdf:type owl:Class ;
            rdfs:comment "An entity is a physical, digital, conceptual, or other kind of thing with some fixed aspects; entities may be real or imaginary."@en ;
            rdfs:label "Entity"@en .


###  http://www.w3.org/ns/prov#Plan
prov:Plan rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          rdfs:comment "A plan is an entity that represents a set of actions or steps intended by one or more agents to achieve some goals."@en ;
          rdfs:label "Plan"@en .


###  http://www.w3.org/ns/prov#Role
prov:Role rdf:type owl:Class ;
          rdfs:comment "A role is the function of an entity or agent with respect to an activity, in the context of a usage, generation, invalidation, association, start, and end."@en ;
          rdfs:label "Role"@en .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
            rdfs:subClassOf prov:Agent ;
            rdfs:comment "The Person class represents people. Something is a Person if it is a person. We don't nitpic about whether they're alive, dead, real, or imaginary. The Person class is a sub-class of the Agent class, since all people are considered 'agents' in FOAF."@en ;
            rdfs:label "Person"@en .


