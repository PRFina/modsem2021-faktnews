#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/terms/Creator
dcterms:Creator rdf:type owl:Class .


###  http://schema.org/CreativeWork
schema:CreativeWork rdf:type owl:Class ;
                    rdfs:subClassOf prov:Entity ;
                    rdfs:comment "The most generic kind of creative work, including books, movies, photographs, software programs, etc."@en ;
                    rdfs:label "Creative Work"@en .


###  http://www.modsem.org/fakt-news#AlternativesList
:AlternativesList rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/set.owl#Set> ;
                  rdfs:comment "A list of claims from which the fact-checker chooses the claim to verify."@en ;
                  rdfs:label "Alternatives List"@en .


###  http://www.modsem.org/fakt-news#Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf prov:Agent ;
        rdfs:comment "An author of a digital document. It could be for example a Fact-Checker or a Claim Author."@en ;
        rdfs:label "Author"@en .


###  http://www.modsem.org/fakt-news#Capacity
:Capacity rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          rdfs:comment "The official position or function that somebody has."@en ;
          rdfs:label "Capacity"@en ;
          skos:altLabel "Function"@en .


###  http://www.modsem.org/fakt-news#Claim
:Claim rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :claimCreator ;
                             owl:someValuesFrom :Claimant
                           ] ;
       rdfs:subClassOf schema:CreativeWork ;
       rdfs:comment "A statement by an entity about a topic."@en ;
       rdfs:label "Claim"@en ;
       skos:altLabel "Statement"@en .


###  http://www.modsem.org/fakt-news#ClaimEvaluationActivity
:ClaimEvaluationActivity rdf:type owl:Class ;
                         rdfs:subClassOf prov:Activity ;
                         rdfs:comment "Last activity of fact checking process. In this activity fact checkers produces a piece of work, the review, about one or more claims supported by one or more piece of evidences."^^xsd:string ;
                         rdfs:label "Claim Evaluation Activity"@en .


###  http://www.modsem.org/fakt-news#Claimant
:Claimant rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :create ;
                                owl:someValuesFrom :Claim
                              ] ;
          rdfs:subClassOf prov:Agent ;
          rdfs:comment "The entity who state the claim."@en ;
          rdfs:label "Claimant"@en .


###  http://www.modsem.org/fakt-news#DeceptiveEditing
:DeceptiveEditing rdf:type owl:Class ;
                  rdfs:subClassOf :Judgment ;
                  rdfs:comment "The claim content presents facts that has been edited and rearranged in such way that ​likely to make the receiver believe something that is not true."@en ;
                  rdfs:label "Deceptive Editing"@en .


###  http://www.modsem.org/fakt-news#DirectSource
:DirectSource rdf:type owl:Class ;
              rdfs:subClassOf :EvidenceSource ;
              rdfs:comment "The direct source that states the fact."@en ;
              rdfs:label "Direct Source"@en .


###  http://www.modsem.org/fakt-news#Doctoring
:Doctoring rdf:type owl:Class ;
           rdfs:subClassOf :MaliciousTransfromation ;
           rdfs:comment "Altering the content of a claim adding or deleting visual, audio or textual information can deceive the receiver."@en ;
           rdfs:label "Doctoring"@en .


###  http://www.modsem.org/fakt-news#Evidence
:Evidence rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          rdfs:comment "An evidence about a claim. Used in the verification stage of the fact-checking process."@en ;
          rdfs:label "Evidence"@en .


###  http://www.modsem.org/fakt-news#EvidenceSource
:EvidenceSource rdf:type owl:Class ;
                rdfs:subClassOf prov:Agent ;
                rdfs:comment """The source of the facts. It could be the entity that states the fact itself or reports it.

TODO: questa classe potrebbe essere una classe definita ottenuta come disjoint union di direct e indirect source"""@en ;
                rdfs:label "Evidence Source"@en .


###  http://www.modsem.org/fakt-news#ExternalSuggestion
:ExternalSuggestion rdf:type owl:Class ;
                    rdfs:subClassOf :Suggestion ;
                    rdfs:comment "External Suggestion - A suggestion from the Reader or from some Recomender System."@en .


###  http://www.modsem.org/fakt-news#Fabrication
:Fabrication rdf:type owl:Class ;
             rdfs:subClassOf :MaliciousTransfromation ;
             rdfs:comment "Using Artificial Intelligence to create high-quality content as fake language content, fake images, fake video, simulates audio and convincingly swaps out background images."@en ;
             rdfs:label "Fabrication"@en .


###  http://www.modsem.org/fakt-news#FactChecker
:FactChecker rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :create ;
                                   owl:someValuesFrom :Review
                                 ] ;
             rdfs:subClassOf prov:Agent ;
             rdfs:comment "The entity that produces a review of a claim given the claim itself and the evidence supporting it."@en ;
             rdfs:label "Fact Checker"@en .


###  http://www.modsem.org/fakt-news#FactCheckingProcess
:FactCheckingProcess rdf:type owl:Class ;
                     rdfs:subClassOf prov:Plan .


###  http://www.modsem.org/fakt-news#IndirectSource
:IndirectSource rdf:type owl:Class ;
                rdfs:subClassOf :EvidenceSource ;
                rdfs:comment "The source of supporting facts. These are essential in order to review the original claim"@en ;
                rdfs:label "Indirect Source"@en ;
                skos:altLabel "Supporting Fact"^^xsd:string .


###  http://www.modsem.org/fakt-news#InternalSuggestion
:InternalSuggestion rdf:type owl:Class ;
                    rdfs:subClassOf :Suggestion ;
                    rdfs:comment "Internal Suggestion - A suggestion from the Staff."@en .


###  http://www.modsem.org/fakt-news#Isolation
:Isolation rdf:type owl:Class ;
           rdfs:subClassOf :MissingContext ;
           rdfs:comment "Sharing a brief excerpt  from a longer story creates a false narrative that does not reflect the event as it occurred."@en ;
           rdfs:label "Isolation"@en .


###  http://www.modsem.org/fakt-news#Judgment
:Judgment rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          rdfs:comment "A concise judgemental statement given by the fact checker to motivate the review rating"@en ;
          rdfs:label "Judgment"@en ;
          skos:altLabel "Justification"@en ,
                        "Reason"@en .


###  http://www.modsem.org/fakt-news#MaliciousTransfromation
:MaliciousTransfromation rdf:type owl:Class ;
                         rdfs:subClassOf :Judgment ;
                         rdfs:comment "Part or all of the claim content has been manipulated to transform the content itself."@en ;
                         rdfs:label "Malicious Transformation"@en .


###  http://www.modsem.org/fakt-news#Mention
:Mention rdf:type owl:Class ;
         rdfs:subClassOf prov:Entity ;
         rdfs:comment "A mention about an entity within a review."@en ;
         rdfs:label "Mention"@en .


###  http://www.modsem.org/fakt-news#Misrepresentation
:Misrepresentation rdf:type owl:Class ;
                   rdfs:subClassOf :MissingContext ;
                   rdfs:comment "Presenting unaltered content in an inaccurate manner misrepresents the content itself and misleads the receiver"@en ;
                   rdfs:label "Misrepresentation"@en .


###  http://www.modsem.org/fakt-news#MissingContext
:MissingContext rdf:type owl:Class ;
                rdfs:subClassOf :Judgment ;
                rdfs:comment "The claim content lacks or misstates the context in which events occurred."@en ;
                rdfs:label "Missing Context"@en .


###  http://www.modsem.org/fakt-news#NotClearGeographyContext
:NotClearGeographyContext rdf:type owl:Class ;
                          rdfs:subClassOf :Misrepresentation ;
                          rdfs:comment "The geographic context is incorrect or not clear such as event incorrect location."@en ;
                          rdfs:label "Not Clear Geography Context"@en ;
                          skos:altLabel "Misrepresented Geography Context"@en .


###  http://www.modsem.org/fakt-news#NotClearPeriodContext
:NotClearPeriodContext rdf:type owl:Class ;
                       rdfs:subClassOf :Misrepresentation ;
                       rdfs:comment "The time context is incorrect or not clear such as event incorrect date."@en ;
                       rdfs:label "Not Clear Time Period Context"@en ;
                       skos:altLabel "Misrepresented Time Period Context"@en .


###  http://www.modsem.org/fakt-news#NotClearSubject
:NotClearSubject rdf:type owl:Class ;
                 rdfs:subClassOf :Misrepresentation ;
                 rdfs:comment "The subject entity of the claim is incorrect or not clear."@en ;
                 rdfs:label "Not Clear Subject"@en ;
                 skos:altLabel "Misrepresented Subject"@en .


###  http://www.modsem.org/fakt-news#Omission
:Omission rdf:type owl:Class ;
          rdfs:subClassOf :DeceptiveEditing ;
          rdfs:comment "Editing out large portions from a video and presenting it as a complete narrative, despite missing key elements, produce a skew reality."@en ;
          rdfs:label "Omission"@en .


###  http://www.modsem.org/fakt-news#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf prov:Entity ;
              rdfs:comment "A group of persons grouped together or organized for some purpose or work."@en ;
              rdfs:label "Organization"@en .


###  http://www.modsem.org/fakt-news#QualitativeClaim
:QualitativeClaim rdf:type owl:Class ;
                  rdfs:subClassOf :Judgment ;
                  rdfs:comment "The claim use qualitative information in place of quantitative descriptions that are open to the receiver interpretation."^^xsd:string ;
                  rdfs:label "Qualitative Claim"@en ;
                  skos:example "\"... in my past five years as leader, I have increased employment.\" is a qualitative claim. This could corresponds to a quantitive information as \"In the past five years, employment has risen from 95.0% to 95.05%.\""^^xsd:string .


###  http://www.modsem.org/fakt-news#Rating
:Rating rdf:type owl:Class ;
        rdfs:subClassOf prov:Entity ;
        rdfs:comment "A rating of a review within a known evaluation meter. The rating value can belong to an external rating system."@en ;
        rdfs:label "Rating"@en .


###  http://www.modsem.org/fakt-news#Review
:Review rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :reviewCreator ;
                              owl:someValuesFrom :FactChecker
                            ] ;
        rdfs:subClassOf schema:CreativeWork ;
        rdfs:comment "The final document that contains the review of a claim by a fact-checker."^^xsd:string ;
        rdfs:label "Review"@en .


###  http://www.modsem.org/fakt-news#SelectionActivity
:SelectionActivity rdf:type owl:Class ;
                   rdfs:subClassOf prov:Activity ;
                   rdfs:comment "First activity of fact checking process. In this activity fact checkers select one or more claims from a list of suggested alternatives guided by the selection policy."@en ;
                   rdfs:label "Selection Activity"@en .


###  http://www.modsem.org/fakt-news#SelectionPolicy
:SelectionPolicy rdf:type owl:Class ;
                 rdfs:subClassOf prov:Entity ;
                 rdfs:comment "The claim selection policy of a given entity."@en ;
                 rdfs:label "Selection Policy"@en .


###  http://www.modsem.org/fakt-news#Splicing
:Splicing rdf:type owl:Class ;
          rdfs:subClassOf :DeceptiveEditing ;
          rdfs:comment "Editing together different excerpts fundamentally alters the story that is being told. An example is glueing together quotes without context."@en ;
          rdfs:label "Splicing"@en .


###  http://www.modsem.org/fakt-news#Suggestion
:Suggestion rdf:type owl:Class ;
            rdfs:subClassOf prov:Entity .


###  http://www.modsem.org/fakt-news#VerificationActivity
:VerificationActivity rdf:type owl:Class ;
                      rdfs:subClassOf prov:Activity ;
                      rdfs:comment "In this activity fact checkers searches piece of evidences to verify the validity of a given claim"@en ;
                      rdfs:label "Verification Activity"@en .


###  http://www.w3.org/ns/prov#Activity
prov:Activity rdf:type owl:Class .


###  http://www.w3.org/ns/prov#Agent
prov:Agent rdf:type owl:Class .


###  http://www.w3.org/ns/prov#Entity
prov:Entity rdf:type owl:Class .


###  http://www.w3.org/ns/prov#Plan
prov:Plan rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity .


